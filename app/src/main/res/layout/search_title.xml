<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dj"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <LinearLayout
        android:id="@+id/d3"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="8.0dip">

        <com.salton123.view.IconFontTextView
            android:id="@+id/fy"
            android:layout_width="42.0dip"
            android:layout_height="26.0dip"
            android:layout_marginRight="8.0dip"
            android:background="@drawable/v"
            android:paddingLeft="5.0dip"
            android:scaleType="fitCenter"
            android:text="@string/ks"
            android:textColor="@color/b4"
            android:textSize="18.0dip"
            android:visibility="gone"
            />

        <RelativeLayout
            android:id="@+id/ft"
            android:layout_width="wrap_content"
            android:layout_height="26.0dip"
            android:layout_marginRight="8.0dip"
            android:visibility="gone">

            <com.salton123.view.IconFontTextView
                android:id="@+id/fr"
                android:layout_width="26.0dip"
                android:layout_height="26.0dip"
                android:scaleType="fitCenter"
                android:text="@string/ks"
                android:textSize="18.0dip"
                android:tint="@color/b4" />

            <TextView
                android:id="@+id/fs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:paddingLeft="2.0dip"
                android:paddingRight="2.0dip"
                android:paddingBottom="2.0dip"
                android:textColor="@color/b4"
                android:textSize="6.0dip"
                android:textStyle="bold" />
        </RelativeLayout>

        <com.salton123.view.IconFontTextView
            android:id="@+id/fo"
            android:layout_width="26.0dip"
            android:layout_height="26.0dip"
            android:layout_marginRight="8.0dip"
            android:scaleType="fitCenter"
            android:text="@string/ks"
            android:textSize="18.0dip"
            android:tint="@color/b4"
            android:visibility="gone"
            />

        <com.salton123.view.NvUrlView
            android:id="@+id/fk"
            android:layout_width="fill_parent"
            android:layout_height="30dp"
            android:layout_marginTop="6.0dip"
            android:layout_marginBottom="6.0dip"
            android:layout_weight="1.0"
            android:visibility="visible" />

        <TextView
            android:id="@+id/fm"
            android:layout_width="wrap_content"
            android:layout_height="30.0dip"
            android:layout_gravity="center"
            android:background="@drawable/af"
            android:clickable="true"
            android:gravity="center"
            android:paddingLeft="10.0dip"
            android:paddingRight="10.0dip"
            android:textColor="@color/b6"
            android:textSize="14.0sp"
            android:visibility="gone" />

        <com.salton123.view.IconFontTextView
            android:id="@+id/tvMore"
            android:layout_width="26.0dip"
            android:layout_height="26.0dip"
            android:layout_marginLeft="2.0dip"
            android:layout_marginRight="2.0dip"
            android:gravity="center"
            android:text="@string/if_more"
            android:textColor="@color/color_ffffffff"
            android:textSize="18.0dip"
            />
    </LinearLayout>

    <FrameLayout
        android:id="@+id/d1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center_vertical"
        android:paddingLeft="8.0dip"
        android:visibility="gone" />

    <TextView
        android:id="@+id/fn"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:paddingLeft="50.0dip"
        android:paddingRight="50.0dip"
        android:singleLine="true"
        android:textColor="@color/b6"
        android:textSize="18.0sp"
        android:visibility="gone" />

    <ProgressBar
        android:id="@+id/fl"
        style="?android:progressBarStyleHorizontal"
        android:layout_width="fill_parent"
        android:layout_height="2.0dip"
        android:layout_alignParentBottom="true"
        android:max="100"
        android:progress="0"
        android:progressDrawable="@drawable/b5" />
</RelativeLayout>